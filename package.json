{
  "name": "tms-website",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "dev": "yarn install && vue-cli-service serve --mode dev",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy_dev": "vue-cli-service build --mode dev && node upload -e dev",
    "deploy_beta": "vue-cli-service build --mode beta && node upload -e beta",
    "deploy_prod": "vue-cli-service build"
  },
  "dependencies": {
    "ali-oss": "^6.0.1",
    "axios": "^0.18.0",
    "clipboard": "^2.0.4",
    "ema-proxy": "^1.0.1",
    "intersection-observer": "^0.5.1",
    "iview": "^3.0.0",
    "js-cookie": "^2.2.0",
    "number-precision": "^1.2.1",
    "photoswipe": "^4.1.2",
    "printd": "^1.0.1",
    "qrcodejs2": "^0.0.2",
    "vue": "^2.5.17",
    "vue-analytics": "^5.16.0",
    "vue-clipboard2": "^0.2.1",
    "vue-count-to": "^1.0.13",
    "vue-echarts": "^3.1.3",
    "vue-lazyload": "^1.2.6",
    "vue-meta": "^1.5.0",
    "vue-router": "^3.0.1",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4",
    "vuex-router-sync": "^5.0.0",
    "ydd_area": "^1.1.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^3.0.4",
    "cross-env": "^5.2.0",
    "easy-ftp": "^0.3.44",
    "lint-staged": "^7.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.17"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore",
        "revert"
      ],
      "scope": {
        "required": false,
        "allowed": [
          "*"
        ],
        "validate": false,
        "multiple": false
      },
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "请使用正确的git message参考ReadMe中的说明",
      "autoFix": false
    }
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
